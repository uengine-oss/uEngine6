<section><row-layout name="TroubleReport" alias="TroubleReport" is_multidata_mode="true" v-model="formValues" v-slot="slotProps"><div v-for="(item, index) in slotProps.modelValue" :key="index"><row-layout-item-head :index="index" v-on:on_delete_item="slotProps.deleteItem(index)"></row-layout-item-head><div class="row"><div class="col-sm-12"><text-field name="TroubleName" alias="제목" type="text" disabled="false" v-model="item['TroubleName']"></text-field><text-field name="TroubleContent" alias="고장내용" type="text" disabled="false" v-model="item['TroubleContent']"></text-field><text-field name="TroubleReporter" alias="신고자" type="text" disabled="false" v-model="item['TroubleReporter']"></text-field>   <text-field name="TroubleWorker" alias="담당자" type="text" disabled="false" v-model="item['TroubleWorker']"></text-field>   <text-field name="dueDate" alias="처리기한" type="date" disabled="false" v-model="item['dueDate']"></text-field><radio-field name="TroubleType" alias="TroubleType" disabled="false" items="[{'trouble':'고장'},{'complaints':'민원'}]" is_dynamic_load="false" dynamic_load_url="" dynamic_load_key_json_path="" dynamic_load_value_json_path="" v-model="item['TroubleType']"></radio-field><file-field name="file" alias="첨부파일" disabled="false" v-model="item['file']"></file-field>   <textarea-field name="TroubleClear" alias="처리내용" rows="5" disabled="false" v-model="item['TroubleClear']"></textarea-field></div></div></div></row-layout></section>